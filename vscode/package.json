{
    "name": "dpscript",
    "displayName": "DPScript Language Support",
    "description": "Language support for DPScript",
    "version": "0.0.1",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/RedstoneWizard08/DPScript"
    },
    "authors": [
        "RedstoneWizard08"
    ],
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dpscript",
                "aliases": [
                    "dps",
                    "dpscript"
                ],
                "extensions": [
                    ".dps"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dpscript",
                "scopeName": "source.dpscript",
                "path": "./syntaxes/dpscript.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "fmt": "prettier --write .",
        "vspack": "vsce package"
    },
    "devDependencies": {
        "@vscode/vsce": "^3.1.1",
        "prettier": "^3.3.3"
    }
}
